    {
      "id": 600,
      "title": "Guard Decision Layer",
      "type": "row",
      "collapsed": true,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 66 },
      "panels": [
        {
          "id": 17,
          "title": "DECISION | Request Rate (layer active)",
          "description": "guard_decision_requests_total — decision layer enabled iken değerlendirilen request sayısı. OpsGuard deny path'te bu counter artmamalı.",
          "type": "timeseries",
          "gridPos": { "h": 8, "w": 8, "x": 0, "y": 67 },
          "datasource": { "uid": "$datasource", "type": "prometheus" },
          "targets": [
            {
              "expr": "sum(rate(ptf_admin_guard_decision_requests_total{job=~\"$job\"}[5m]))",
